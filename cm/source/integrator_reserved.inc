!integrator_reserved.inc
C     The reserved indices for the integer and real control arrays for
C     the cell model integration routines.

C     The real control array indices
      INTEGER SIZE_REAL_CONTROL
      PARAMETER(SIZE_REAL_CONTROL=3)
      INTEGER ABSOLUTE_ERROR,RELATIVE_ERROR,MAXIMUM_STEP_SIZE
      PARAMETER(ABSOLUTE_ERROR=1)
      PARAMETER(RELATIVE_ERROR=2)
      PARAMETER(MAXIMUM_STEP_SIZE=3)

C     The integer control array indices
      INTEGER SIZE_INTEGER_CONTROL
      PARAMETER(SIZE_INTEGER_CONTROL=10)
      INTEGER ERROR_CODE,MAXIMUM_ITERATIONS,MAXIMUM_ORDER,ERROR_TYPE,
     '  EXTEND_INTERVAL,USE_ROUNDING_CONTROL,INTEGER_WORK_SIZE,
     '  REAL_WORK_SIZE,GET_WORK_SIZES,SET_DEFAULT_VALUES
      PARAMETER(ERROR_CODE=1)
      PARAMETER(MAXIMUM_ITERATIONS=2)
      PARAMETER(MAXIMUM_ORDER=3)
      PARAMETER(ERROR_TYPE=4)
      PARAMETER(EXTEND_INTERVAL=5)
      PARAMETER(USE_ROUNDING_CONTROL=6)
      PARAMETER(INTEGER_WORK_SIZE=7)
      PARAMETER(REAL_WORK_SIZE=8)
      PARAMETER(GET_WORK_SIZES=9)
      PARAMETER(SET_DEFAULT_VALUES=10)

C     Error code values
      INTEGER INCREASE_REAL_WORK,INCREASE_INTEGER_WORK,RHS_ERROR
      PARAMETER(INCREASE_REAL_WORK=-1)
      PARAMETER(INCREASE_INTEGER_WORK=-2)
      PARAMETER(RHS_ERROR=-3)
