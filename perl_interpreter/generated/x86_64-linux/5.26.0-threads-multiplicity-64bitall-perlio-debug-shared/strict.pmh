"package strict;\n"
"\n"
"\n"
"$strict::VERSION = \"1.01\";\n"
"\n"
"my %bitmask = (\n"
"refs => 0x00000002,\n"
"subs => 0x00000200,\n"
"vars => 0x00000400\n"
");\n"
"\n"
"sub bits {\n"
"    my $bits = 0;\n"
"    foreach my $s (@_){ $bits |= $bitmask{$s} || 0; };\n"
"    $bits;\n"
"}\n"
"\n"
"sub import {\n"
"    shift;\n"
"    $^H |= bits(@_ ? @_ : qw(refs subs vars));\n"
"}\n"
"\n"
"sub unimport {\n"
"    shift;\n"
"    $^H &= ~ bits(@_ ? @_ : qw(refs subs vars));\n"
"}\n"
